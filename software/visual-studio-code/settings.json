{
    "window.zoomLevel": 1,
    "editor.fontSize": 16,
    "editor.fontFamily": "Monaco",
    "editor.tabSize": 2,
    "editor.lineHeight": 26,
    "editor.cursorSurroundingLines": 5,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80
    ],
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.commandPalette.preserveInput": true,
    "workbench.quickOpen.preserveInput": true,
    "files.associations": {
        "bashrc": "shellscript"
    },
    "explorer.sortOrder": "mixed",
    "ruby.useLanguageServer": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vsintellicode.java.completionsEnabled": false,
    "vsintellicode.sql.completionsEnabled": false,
    "workbench.activityBar.visible": false,
    "files.exclude": {
        "**/package-lock.json": true
    },
    "search.smartCase": true,
    "terminal.integrated.scrollback": 100000,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.startupEditor": "newUntitledFile",
    "files.insertFinalNewline": true,
    "markdown.links.openLocation": "beside",
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.macOptionIsMeta": true,
    "workbench.colorCustomizations": {
        "terminal.background": "#000000",
        "terminal.foreground": "#c7c7c7",
        "terminal.selectionBackground": "#454d95",
        "terminal.ansiBlack": "#616161",
        "terminal.ansiBlue": "#a5d5fe",
        "terminal.ansiCyan": "#d0d1fe",
        "terminal.ansiGreen": "#b4fa72",
        "terminal.ansiMagenta": "#ff8ffd",
        "terminal.ansiRed": "#ff8272",
        "terminal.ansiWhite": "#f1f1f1",
        "terminal.ansiYellow": "#fefdc2",
        "terminal.ansiBrightBlack": "#616161",
        "terminal.ansiBrightBlue": "#a5d5fe",
        "terminal.ansiBrightCyan": "#d0d1fe",
        "terminal.ansiBrightGreen": "#b4fa72",
        "terminal.ansiBrightMagenta": "#ff8ffd",
        "terminal.ansiBrightRed": "#ff8272",
        "terminal.ansiBrightWhite": "#f1f1f1",
        "terminal.ansiBrightYellow": "#fefdc2"
    },
    "workbench.panel.defaultLocation": "right",
    "update.showReleaseNotes": false,
    "terminal.integrated.rendererType": "dom",
    "search.showLineNumbers": true,
    "search.useGlobalIgnoreFiles": true,
    "search.quickOpen.includeHistory": false
}
