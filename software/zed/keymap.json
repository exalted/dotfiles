      "ctrl-tab":"pane::ActivateNextItem",
      "ctrl-shift-tab":"pane::ActivatePreviousItem"
    }
  },
  {
    "context":"Editor",
    "bindings":{
      "alt-up": "editor::SelectLargerSyntaxNode",
      "alt-down": "editor::SelectSmallerSyntaxNode",
