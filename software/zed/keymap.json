      "ctrl-cmd-up": "editor::MoveLineUp",
      "ctrl-cmd-down": "editor::MoveLineDown"
    }
  },
  {
    "context":"ProjectSearchView",
    "bindings":{
      "alt-cmd-s":"workspace::SaveAll"
    }
  },
  {
    "context":"Pane",
    "bindings":{
      "alt-cmd-s":"workspace::SaveAll"
    }
  }
